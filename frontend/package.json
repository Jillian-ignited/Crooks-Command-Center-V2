{
  "name": "crooks-command-center-frontend",
  "version": "2.0.5",
  "private": true,
  "description": "Next.js frontend for Crooks Command Center (static-exported and served by FastAPI).",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o out",
    "export": "next export -o out",
    "copy:static": "rm -rf ../backend/static/site && mkdir -p ../backend/static/site && cp -R out/* ../backend/static/site/",
    "start": "next start"
  },
  "dependencies": {
    "next": "14.2.33",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
